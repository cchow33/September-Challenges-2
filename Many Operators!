// Some basic arithmetic operators are +, =, *, / and %. Given three parameters, num1, num2, num3 and an operator. Use the operator on number 1 and 2


function operate(num1, num2, operator){
    return `${num1} ${operator} ${num2}`
}

console.log(operate(1, 2, "+")) // 2
console.log(operate(7, 10, "-")) // -3
console.log(operate(20, 10, "%")) // 0
